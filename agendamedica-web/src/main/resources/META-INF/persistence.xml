<?xml version="1.0" encoding="UTF-8"?>
<persistence version="1.0" xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_1_0.xsd">
    <persistence-unit name="Transaccion" transaction-type="JTA">
        <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
        <jta-data-source>jdbc/agendamedica</jta-data-source>
        <class>cl.duoc.agendamedica.modelo.Cliente</class>
        <class>cl.duoc.agendamedica.modelo.Doctor</class>
        <class>cl.duoc.agendamedica.modelo.Usuario</class>
        <properties>
            <property name="eclipselink.target-server" value="WebLogic_10" />
            <property name="eclipselink.logging.level" value="FINEST" />
        </properties>
    </persistence-unit>
</persistence>